{
  "name": "cospace-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "init:test": "cospace init test",
    "purge": "cospace purge",
    "list-pkgs": "pnpm ls -r --depth -1"
  },
  "packageManager": "pnpm@6.32.1",
  "devDependencies": {
    "cospace": "workspace:*"
  },
  "pnpm": {
    "overrides": {
      "cospace": "workspace:*"
    },
    "prefer-workspace-packages": true,
    "link-workspace-packages": "deep"
  }
}
